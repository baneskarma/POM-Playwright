{
  "singleQuote": true,
  "parser": "flow",
  "semi": true,
  "trailingComma": "es5",
  "object-curly-newline": [{ "multiline": true, "consistent": true }],
  "printWidth": 10000,
  "newline-per-chained-call": true,
  "array-spacing": true,
  "array-bracket-newline": true,
  "bracketSpacing": true,
  "tabWidth": 4,
  "useTabs": true,
  "editor.formatOnSave": true,
  "function-call-argument-newline": true,
  "space-in-parens": true,
  "quoteProps": "as-needed"
}
